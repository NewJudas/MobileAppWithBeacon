<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobilTest.Views.RegStudentPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Skriv din RegistreringsKod" FontSize="Large" TextColor="Purple"/>

            <Entry Text="{Binding Id}" TextColor="Blue"/>

            <Button Text="Save" Clicked="Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>